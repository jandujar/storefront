<div id="goods-store" data-role="page">
    <img src="{{pages.goods.background}}" class="background">
    <div class="header" style="background-image: url('{{pages.goods.headerBackground}}');">
        <div class="title">
            <img src="{{pages.goods.title}}">
        </div>
        <div class="balance-container">
            <div class="balance">
                <img src="{{pages.goods.balance}}">
                {{#each currencies}}
                <div class="container">
                    <label>{{this.balance}}</label>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
    <div class="items-container" style="background-image: url('{{pages.goods.list.background}}');">
    </div>
    <div class="footer">
        <div>
            <img src="{{pages.goods.backButton}}" class="leave-store">
        </div>
        <div>
            <img src="{{pages.goods.moreCurrencyButton}}" class="currency buy-more">
        </div>
    </div>
</div>
<div id="currency-store" data-role="page">
    <img src="{{pages.currencyPacks.background}}" class="background">
    <div class="header" style="background-image: url('{{pages.currencyPacks.headerBackground}}');">
        <div class="title">
            <img src="{{pages.currencyPacks.title}}">
        </div>
        <div class="balance-container">
            <div class="balance">
                <img src="{{pages.currencyPacks.balance}}">
                {{#each currencies}}
                <div class="container">
                    <label>{{this.balance}}</label>
                </div>
                {{/each}}
            </div>
        </div>
    </div>
    <div class="items-container" style="background-image: url('{{pages.currencyPacks.list.background}}');">
    </div>
    <div class="footer">
        <div>
            <img src="{{pages.currencyPacks.backButton}}" class="back">
        </div>
    </div>

</div>
