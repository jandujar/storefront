<div class="visual">
    <img src="examples/muffinRush/Mapit.png" class="background">
    <img src="{{imgFilePath}}" class="foreground">
</div>
<div class="content">
    <h1>{{name}}</h1>
    <div class="description">{{description}}</div>
    <div class="box-horizontal">
        <div class="balance">Owned: <label>{{balance}}</label></div>
    </div>
</div>
<div class="price">
    <img src="{{currency.imgFilePath}}" class="currency">
    <label>{{price}}</label>
</div>
